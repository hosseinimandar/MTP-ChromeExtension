{
  "manifest_version": 3,
  "name": "Multi Tier Proxier",
  "version": "2.0",
  "description": "A dynamic and secure proxy extension.",
  "permissions": [
    "proxy",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}